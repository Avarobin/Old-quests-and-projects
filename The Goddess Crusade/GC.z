			if ( Confirmed == 0 ) {
				if ( Link->PressUp ) {
					Game->PlaySound(5);
					Selected --;
					if ( Selected < 0 )
						Selected = 2;
				}
				if ( Link->PressDown ) {
					Game->PlaySound(5);
					Selected ++;
					if ( Selected > 2 )
						Selected = 0;
				}
				
				if ( Link->PressStart || Link->PressA ) {
					Game->PlaySound(68);
					Confirmed = 1;
				}
			}
			
			// text
			
			if ( Selected == 0 && (Confirmed == 0 || Blinking < 2) )
				colour = ColourText2;
			else
				colour = ColourText1;
			Screen->DrawString(7, 128, MainFrameY+20 + 16*0, FONT_Z1, colour, -1, TF_CENTERED, TXT_Continue, OP_OPAQUE);
			
			if ( Selected == 1 && (Confirmed == 0 || Blinking < 2) )
