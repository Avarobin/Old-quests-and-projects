	void Dodongo_Waitframes(ffc this, npc ghost, int num){
		for (int i = num; i > 0; --i)
			Dodongo_Waitframe(this, ghost);
	}
	void Dodongo_SetHitboxHorizontal(ffc this, npc ghost){
