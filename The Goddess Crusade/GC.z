	Ghost_Y = y;
	Ghost_SetAllDefenses(ghost, NPCDT_NONE);
	ghost->CollDetection = true;
	ghost->DrawXOffset = 0;
	Boss_FaceLink(this, ghost);
	Boss_Ghi_DiamondExplosion(Ghost_X, Ghost_Y);
